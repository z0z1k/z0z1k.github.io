function savera() {
    var apiname = $("#save")["val"]();
	var aname = $("#core")["val"]();
	var fname = $("#lir")["val"]();
	var gmoney = $("#cash")["val"]();
	var promokey = $("#promoid")["val"]();
	var phero = $("#photq")["val"]();
	var phid = $("#useqrId")["val"]();
	var th = $("#prava")["val"]();
     $.ajax({
        type: "POST",
        url: "/system/function.php",
        data: {
			apiname: apiname,
			aname: aname,
			gmoney: gmoney,
			fname: fname,
			promokey: promokey,
			phero: phero,
			phid: phid,
			th: th
		},
